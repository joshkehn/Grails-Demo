public class Bar
{
	public static void main(String [] args)
	{
		Foo b = new Foo(10);
		for(int i = 0; i < 10; i++)
		{
			if(i % 3 == 0)
			{
				b.inc();
			}
			else
			{
				b.dec();
			}
			System.out.println(b);
		}
	}
}
public class Foo
{
	int self;
	public Foo(int val)
	{
		self = val;
	}
	
	public void inc()
	{
		self++;
	}
	
	public void dec()
	{
		self--;
	}
	
	public int getSelf()
	{
		return self;
	}
	
	public int toString()
	{
		return self;
	}
}